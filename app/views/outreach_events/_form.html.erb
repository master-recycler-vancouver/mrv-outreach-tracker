<%= form_for @outreach_event do |form| %>
 
  <% if @outreach_event.errors.any? %>
    <% #TODO %>
  <% end %>
 
  <p>
    <%= form.label :start_time %><br>
    <%= form.datetime_field :start_time %>
  </p>

  <p>
    <%= form.label :end_time %><br>
    <%= form.datetime_field :end_time %>
  </p>

  <p>
    <%= form.label :description %><br>
    <%= form.text_area :description %>
  </p>

  <p>
    <%= form.label :duration_in_minutes %><br>
    <%= form.number_field :duration_in_minutes %>
  </p>

  <p>
    <%= form.label :people_reached %><br>
    <%= form.number_field :people_reached %>
  </p>

  <p>
    <%= form.label :notes %><br>
    <%= form.text_area :notes %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
 
<% end %>